{
  "name": "optimism-starter",
  "version": "0.0.4",
  "private": true,
  "scripts": {
    "anvil": "source .env && anvil --fork-url $ANVIL_FORK_URL --chain-id $ANVIL_CHAIN_ID",
    "deploy": "source .env && forge script contracts/script/DeployPassportNFT.s.sol --rpc-url $FORGE_RPC_URL --broadcast --private-key $FORGE_PRIVATE_KEY --verify --etherscan-api-key $ETHERSCAN_API_KEY",
    "dev": "npm run wagmi && vite",
    "dev:foundry": "npm run dev & wagmi generate --watch & npm run anvil",
    "build": "npm run wagmi && vite build",
    "build:production": "vite build",
    "preview": "vite preview",
    "lint": "prettier --write \"**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "wagmi": "wagmi generate"
  },
  "dependencies": {
    "@eth-optimism/atst": "^0.0.0-20230306200511",
    "@eth-optimism/contracts-bedrock": "^0.12.1",
    "@eth-optimism/contracts-periphery": "^1.0.7",
    "@eth-optimism/sdk": "0.0.0-20230301065104",
    "@ethereum-attestation-service/eas-contracts": "^1.0.0-beta.0",
    "@googlemaps/js-api-loader": "^1.16.2",
    "@openzeppelin/contracts": "4.7.3",
    "@openzeppelin/contracts-upgradeable": "4.7.3",
    "@rainbow-me/rainbowkit": "^1.0.7",
    "@wagmi/cli": "^1.3.0",
    "@worldcoin/idkit": "^0.5.1",
    "buffer": "^6.0.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-places-autocomplete": "^7.3.0",
    "react-qr-code": "^2.0.11",
    "react-router-dom": "^6.15.0",
    "util": "^0.12.4",
    "viem": "^1.4.2",
    "wagmi": "^1.3.9"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.4",
    "@types/google.maps": "^3.53.6",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "daisyui": "^3.5.1",
    "postcss": "^8.4.27",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.3.3",
    "tailwindcss-vite-plugin": "^0.0.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4"
  }
}
